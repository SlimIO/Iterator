{
    "name": "@slimio/iterator",
    "version": "1.0.0",
    "description": "Iterators utils",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && ava --verbose",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SlimIO/Iterator.git"
    },
    "keywords": [
        "iterator",
        "generator",
        "utils",
        "helper"
    ],
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SlimIO/Iterator/issues"
    },
    "homepage": "https://github.com/SlimIO/Iterator#readme",
    "dependencies": {
        "@slimio/is": "^1.5.0",
        "is-stream": "^2.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^3.0.2",
        "@slimio/psp": "^0.9.2",
        "@types/node": "^12.12.8",
        "ava": "^3.0.0",
        "cross-env": "^6.0.3",
        "eslint": "^6.6.0",
        "husky": "^3.0.9",
        "jsdoc": "^3.6.2",
        "nyc": "^14.1.1",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
